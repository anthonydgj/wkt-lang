{
  "name": "wkt-lang",
  "version": "0.2.5",
  "description": "WKT preprocessor for geospatial data scripting",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anthonydgj/wkt-lang.git"
  },
  "files": [
    "dist",
    "examples"
  ],
  "type": "commonjs",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "scripts": {
    "build": "tsup --config ./tsup.config.ts",
    "test": "jest"
  },
  "author": "AnthonyDGJ",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/wellknown": "^0.5.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@turf/turf": "^6.5.0",
    "chalk": "^4.1.2",
    "fs": "^0.0.1-security",
    "ohm-js": "^17.1.0",
    "tslib": "^2.6.2",
    "wellknown": "^0.5.0",
    "yargs": "^17.7.2"
  },
  "keywords": [
    "wkt",
    "well-known text",
    "ohm",
    "dsl",
    "domain-specific language",
    "geospatial"
  ]
}
